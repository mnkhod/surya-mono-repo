{
  "name": "surya-admin",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",

    "vercelBuild": "dotenv -e=.env.local -- prisma generate --schema=./prisma/prodSchema.prisma && next build",

    "prismaDev": "dotenv -e .env.local prisma migrate dev",

    "generate": "npx dotenv -e .env.local -- prisma generate",
    "prodGenerate": "dotenv -e .env.local -- prisma generate --schema=./prisma/prodSchema.prisma",

    "dbSeed": "dotenv -e .env.local prisma db seed",
    "dbProdSeed": "dotenv -e .env.local -- prisma db seed --schema=./prisma/prodSchema.prisma",

    "dbPush": "dotenv -e .env.local -- prisma db push --schema=./prisma/prodSchema.prisma",
    "dbReset": "dotenv -e .env.local -- prisma migrate reset --schema=./prisma/prodSchema.prisma",

    "dbMigrate": "dotenv -e .env.local prisma migrate dev",
    "studio": "dotenv -e .env.local prisma studio",
    "prodStudio": "dotenv -e .env.local --  prisma studio --schema=./prisma/prodSchema.prisma"
  },
  "dependencies": {
