    "@types/node": "20.2.5",
    "@types/react": "18.2.7",
    "@types/react-big-calendar": "^1.6.4",
    "@types/react-dom": "18.2.4",
    "ag-grid-community": "^29.3.5",
    "ag-grid-react": "^29.3.5",
    "autoprefixer": "10.4.14",
